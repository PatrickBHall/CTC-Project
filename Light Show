int yellow = 12;
int green = 11;
int red = 10;
int blue = 9;

void setup(){
  pinMode(yellow, OUTPUT);
  pinMode(green, OUTPUT);
  pinMode(red, OUTPUT);
  pinMode(blue, OUTPUT);
}
void loop(){
  //from 1-4
  delay(1000);
  digitalWrite(9, HIGH);
  digitalWrite(12, HIGH);
  delay(100);
  digitalWrite(12, LOW);
  digitalWrite(9, LOW);
  
